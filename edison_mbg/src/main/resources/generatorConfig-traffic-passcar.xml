<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
<!--    <properties resource="generator.properties"/>-->
    <properties resource="application-traffic.properties"/>
    <context id="MySqlContext" targetRuntime="MyBatis3" defaultModelType="flat">
        <property name="beginningDelimiter" value="`"/>
        <property name="endingDelimiter" value="`"/>
        <property name="javaFileEncoding" value="UTF-8"/>
        <!-- 为模型生成序列化方法-->
        <plugin type="org.mybatis.generator.plugins.SerializablePlugin"/>
        <!-- 为生成的Java模型创建一个toString方法 -->
<!--        <plugin type="org.mybatis.generator.plugins.ToStringPlugin"/>-->
        <!--生成mapper.xml时覆盖原文件-->
        <plugin type="org.mybatis.generator.plugins.UnmergeableXmlMappersPlugin" />
        <!-- Mapper注解插件 -->
        <plugin type="com.itfsw.mybatis.generator.plugins.MapperAnnotationPlugin">
            <!-- @Mapper 默认开启 -->
            <property name="@Mapper" value="true"/>
            <!-- @Repository 默认关闭，开启后解决IDEA工具@Autowired报错 -->
            <property name="@Repository" value="true"/>
        </plugin>
        <commentGenerator type="com.edison.springbootdemo.plugins.CommentGenerator">
            <!-- 是否去除自动生成的注释 true：是 ： false:否 -->
            <property name="suppressAllComments" value="true"/>
            <property name="suppressDate" value="true"/>
            <property name="addRemarkComments" value="true"/>
            <property name="addSwaggerApiProperty" value="true"/><!--是否开启swagger注解开关-->
        </commentGenerator>

        <jdbcConnection driverClass="${spring.datasource.driverClassName}"
                        connectionURL="${spring.datasource.url}"
                        userId="${spring.datasource.username}"
                        password="${spring.datasource.password}">
            <!--解决mysql驱动升级到8.0后不生成指定数据库代码的问题-->
            <property name="nullCatalogMeansCurrent" value="true" />
            <!--解决ORACLE数据库无法获取注释的问题-->
            <property name="remarksReporting" value="true"/>
        </jdbcConnection>

        <javaTypeResolver type="com.edison.springbootdemo.plugins.MyJavaTypeResolverImpl">
            <property name="forceBigDecimals" value="false"/><!--整型字段是否强制转为bigdecimal-->
            <property name="smallintToInteger" value="true"/> <!--smallint类型的字段是否也要转为Integer-->
        </javaTypeResolver>

        <javaModelGenerator targetPackage="com.cmnit.traffic.passcar.domain.entity.traffic" targetProject="${target.project.path}">
            <property name="enableSubPackages" value="false"/>
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="com.cmnit.traffic.passcar.infrastructure.mapper.traffic" targetProject="${target.project.path}">
            <property name="enableSubPackages" value="false"/>
        </sqlMapGenerator>

        <javaClientGenerator type="XMLMAPPER" targetPackage="com.cmnit.traffic.passcar.infrastructure.mapper.traffic"
                             targetProject="${target.project.path}">
            <property name="enableSubPackages" value="false"/>
        </javaClientGenerator>

        <!--生成全部表tableName设为%-->
<!--        <table tableName="%">-->
<!--            &lt;!&ndash;generatedKey是为了在插入时返回自增主键id的值&ndash;&gt;-->
<!--&lt;!&ndash;            <generatedKey column="id" sqlStatement="MySql" identity="true"/>&ndash;&gt;-->
<!--        </table>-->
        <!--<table tableName="EM_INFO" domainObjectName="EmInfo"
                                    enableCountByExample="false"
                                    enableDeleteByExample="false"
                                    enableUpdateByExample="false"
                                    enableSelectByExample="false"></table>-->
    <!--    <table tableName="t_video_information"  enableCountByExample="false"
               enableDeleteByExample="false"
               enableUpdateByExample="false"
               enableSelectByExample="false"
               />-->
       <!-- <table tableName="t_push_device"  enableCountByExample="false"
               enableDeleteByExample="false"
               enableUpdateByExample="false"
               enableSelectByExample="false"
        />-->

        <table tableName="vehicle_congestion_event"  enableCountByExample="false"
               enableDeleteByExample="false"
               enableUpdateByExample="false"
               enableSelectByExample="false"></table>

        <table tableName="vehicle_event_short_msg"  enableCountByExample="false"
               enableDeleteByExample="false"
               enableUpdateByExample="false"
               enableSelectByExample="false"></table>

    </context>
</generatorConfiguration>